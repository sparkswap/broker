{
  "name": "kinesis-broker",
  "script": "broker-daemon/start.js",
  "instances": "1",
  "max_restarts": 4,
  "min_uptime": 5000,
  "watch": [
    "broker-daemon"
  ],
  "env": {
    "NODE_ENV": "development",
    "NODE_PATH": "."
  },
  "watch_options": {
    "usePolling": true,
    "interval": 5007
  }
}
