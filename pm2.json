{
  "name": "sparkswapd",
  "script": "./scripts/start-sparkswapd.sh",
  "instances": "1",
  "max_restarts": 10,
  "exec_interpreter" : "bash",
  "exec_mode"  : "fork_mode",
  "min_uptime": 5000,
  "env": {
    "NODE_ENV": "development",
    "watch_options": {
      "usePolling": true,
      "interval": 5001
    },
    "watch": [
      "broker-daemon",
      "proto"
    ]
  }
}
