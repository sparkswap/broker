{
  "name": "kinesis-broker",
  "script": "./scripts/start-kbd.sh",
  "instances": "1",
  "max_restarts": 10,
  "exec_interpreter" : "bash",
  "exec_mode"  : "fork_mode",
  "min_uptime": 5000,
  "watch": [
    "broker-daemon",
    "proto",
    "node_modules/lnd-engine"
  ],
  "env": {
    "NODE_ENV": "development",
    "NODE_PATH": "."
  },
  "watch_options": {
    "usePolling": true,
    "interval": 5001
  }
}
